<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:layout_marginHorizontal="20dp">

    <TextView
        android:id="@+id/textViewListTanggal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:fontFamily="@font/poppins_semibold"
        android:textSize="24dp"
        android:textColor="@color/colorBlack"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp" />
    <TextView
        android:id="@+id/textViewListKalori"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp"
        android:text="Kalori"
        android:fontFamily="@font/poppins_semibold"
        android:textSize="22dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_alignParentEnd="true"
        android:layout_alignBottom="@+id/textViewListName" />

    <TextView
        android:id="@+id/textViewListKalori1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textViewListName"
        android:fontFamily="@font/poppins_regular"
        android:textSize="14dp"
        android:layout_below="@+id/textViewListKalori"
        android:text="TextView" />
    <TextView
        android:id="@+id/textViewListProtein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textViewListName"
        android:fontFamily="@font/poppins_regular"
        android:textSize="14dp"
        android:layout_below="@+id/textViewListKalori1"
        android:text="TextView" />
    <TextView
        android:id="@+id/textViewListKarbo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textViewListName"
        android:fontFamily="@font/poppins_regular"
        android:textSize="14dp"
        android:layout_below="@+id/textViewListProtein"
        android:text="TextView" />
    <TextView
        android:id="@+id/textViewListLemak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textViewListName"
        android:fontFamily="@font/poppins_regular"
        android:textSize="14dp"
        android:layout_below="@+id/textViewListKarbo"
        android:text="TextView"
        android:layout_marginBottom="10dp"/>
</RelativeLayout>